<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
	version="2.2">
	<application>
		<resource-bundle>
			<base-name>messages</base-name>
			<var>msgs</var>
		</resource-bundle>
	</application>
	<managed-bean>
		<managed-bean-name>settings</managed-bean-name>
		<managed-bean-class>uo.sdi.presentation.BeanSettings</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>viajes</managed-bean-name>
		<managed-bean-class>uo.sdi.presentation.BeanTrips</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>sessionuser</managed-bean-name>
		<managed-bean-class>uo.sdi.presentation.BeanUser</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>login</managed-bean-name>
		<managed-bean-class>uo.sdi.presentation.BeanLogin</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>solicitudes</managed-bean-name>
		<managed-bean-class>uo.sdi.presentation.BeanApplication</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>seat</managed-bean-name>
		<managed-bean-class>uo.sdi.presentation.BeanSeat</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<navigation-rule>
		<from-view-id>/restricted/*</from-view-id>
		<navigation-case>
			<from-action>#{login.cerrarSesion}</from-action>
			<from-outcome>exito</from-outcome>
			<to-view-id>/index.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/index.xhtml</from-view-id>
		<navigation-case>
			<from-action>#{viajes.listado}</from-action>
			<from-outcome>exito</from-outcome>
			<to-view-id>/listadoUsuarioPublico.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>registrarse</from-outcome>
			<to-view-id>/registro.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>#{login.verify}</from-action>
			<from-outcome>exito</from-outcome>
			<to-view-id>/restricted/principal.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>#{login.verify}</from-action>
			<from-outcome>fracaso</from-outcome>
			<to-view-id>/index.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/registro.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>volverLogin</from-outcome>
			<to-view-id>/index.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>#{sessionuser.alta}</from-action>
			<from-outcome>exito</from-outcome>
			<to-view-id>/index.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>#{sessionuser.alta}</from-action>
			<from-outcome>error</from-outcome>
			<to-view-id>/registro.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/restricted/principal.xhtml</from-view-id>
		<navigation-case>
			<from-action>#{viajes.listadoDetalle}</from-action>
			<from-outcome>exito</from-outcome>
			<to-view-id>/restricted/listadoUsuarioValidado.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>#{application.eliminar(var)}</from-action>
			<from-outcome>exito</from-outcome>
			<to-view-id>/restricted/principal.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>verSolicitudes</from-outcome>
			<to-view-id>/restricted/listadoSolicitantesViaje.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>verPasajerosConPlaza</from-outcome>
			<to-view-id>/restricted/listadoPasajerosViaje.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/restricted/listadoUsuarioValidado.xhtml</from-view-id>
		<navigation-case>
			<from-action>#{viajes.cargarViajesUsuario}</from-action>
			<from-outcome>exito</from-outcome>
			<to-view-id>/restricted/principal.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>#{viajes.solicitarPlaza(trip)}</from-action>
			<from-outcome>exito</from-outcome>
			<to-view-id>/restricted/principal.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/restricted/listadoPasajerosViaje.xhtml</from-view-id>
		<navigation-case>
			<from-action>#{viajes.cargarViajesUsuario}</from-action>
			<from-outcome>exito</from-outcome>
			<to-view-id>/restricted/principal.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/restricted/listadoSolicitantesViaje.xhtml</from-view-id>
		<navigation-case>
			<from-action>#{viajes.cargarViajesUsuario}</from-action>
			<from-outcome>exito</from-outcome>
			<to-view-id>/restricted/principal.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
</faces-config>
